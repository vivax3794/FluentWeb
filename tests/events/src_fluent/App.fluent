<template>
    <button id="TARGET" :click="|_, _| *value += 1">{value}</button>

    <span id ="OTHER_TARGET">{other_value}</span>
    <component src="Sub" ;click="|event| *other_value = event.0"></component>

    <input :click="|_, element| dbg!(element.value())"></button>
    <input :click="|event: &InputEvent, _| dbg!(event.input_type())"></button>
</template>

<data>
let mut value: i32 = 0;
let mut other_value: i32 = 0;
</data>

<define>
use super::Sub;
</define>